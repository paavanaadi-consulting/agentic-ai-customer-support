# Local Native Testing Configuration (No Docker)
# Copy this to .env.local for local testing without Docker

# Local Database Configuration (SQLite)
DATABASE_URL=sqlite:///./data/test.db
DB_TYPE=sqlite
DB_PATH=./data/test.db

# Local Message Queue (In-Memory)
KAFKA_ENABLED=false
MESSAGE_QUEUE_TYPE=memory

# Local Cache (In-Memory)
REDIS_ENABLED=false
CACHE_TYPE=memory

# Vector Database (Disabled for basic testing)
VECTOR_DB_ENABLED=false

# MCP Server Configuration (Local Processes)
MCP_DATABASE_ENABLED=true
MCP_DATABASE_TYPE=local
MCP_KAFKA_ENABLED=false
MCP_AWS_ENABLED=false

# A2A Agent Configuration (Local Testing)
A2A_QUERY_AGENT_PORT=8101
A2A_KNOWLEDGE_AGENT_PORT=8102
A2A_RESPONSE_AGENT_PORT=8103
A2A_COORDINATOR_PORT=8104

# LLM Configuration (Optional - add your keys for enhanced testing)
# OPENAI_API_KEY=your_openai_api_key_here
# GEMINI_API_KEY=your_gemini_api_key_here
# ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Test Configuration
TEST_MODE=native
TEST_DATA_PATH=./data/test.db
LOG_LEVEL=INFO
LOG_FILE=./logs/native_test.log

# Native Testing Features
NATIVE_TESTING=true
USE_MOCK_SERVICES=true
CREATE_SAMPLE_DATA=true
ENABLE_PERFORMANCE_TRACKING=true
